{
  "name": "k-bit/laravel-keeping",
  "description": "A Laravel package to use the API of the Keeping time registration tool",
  "type": "library",
  "keywords": ["laravel", "keeping"],
  "require": {
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/support": "~5.5|~6.0|~7.0|~8.0",
    "jenssegers/model": "^1.4"
  },
  "authors": [
    {
      "name": "Xander ten Boden",
      "email": "xander@k-bit.dev"
    }
  ],
  "autoload": {
    "psr-4": {
      "KBit\\LaravelKeeping\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KBit\\LaravelKeeping\\LaravelKeepingServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
